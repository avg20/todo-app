{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "react/prefer-es6-class": 0,
    "react/jsx-indent-props": 0,
    "react/prefer-stateless-function": 0
  }
}